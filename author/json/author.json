{
  "createtypes":["object","image","shape","settings"],
  "settings":[
      {"type":"canvaswidth",  "widgets":{"size":"int"}},
      {"type":"canvasheight", "widgets":{"size":"int"}},
      {"type":"canvasparent", "widgets":{"parent":"text"}}
  ],
  "object":{
    "image":{"widgets":{"id":"text", "group":"arraystrings", "parent.id":"objectdata", "show":"bool", "image":"imagedata", "origin":"text", "position":"posxy","scale":"scale", "testp":"bool", "clicklist":"activitylist", "draglist":"activitylist", "droplist":"activitylist", "truelist":"activitylist"}},
    "shape":{"widgets":{"id":"text", "group":"arraystrings", "show":"bool", "shape":"shapedata", "position":"posxy" },"widgetbank":["actions"] }
  },
  "image":[
    {"type":"imagedata", "widgets":{"id":"text", "url":"filename"}}
  ],
  "shape":[
    {"type":"shapedata", "widgets":{"id":"text", "color":"color", "defaultcolor":"color", "selectcolor":"color"},"widgetbank":["drawcode"]}
  ],
  "drawcode":[
    {"type":"line",        "widgets":{"offset":"xy"}},
    {"type":"arc",         "widgets":{"offset":"xy", "radius":"int","startangle":"int", "endangle":"int", "counterclockwise":"bool"}},
    {"type":"bcurve",      "widgets":{"offseta":"xy", "offsetb":"xy", "offsetc":"xy"}},
    {"type":"linewidth",   "widgets":{"width":"int"}},
    {"type":"fillStyle",   "widgets":{"color":"color"}},
    {"type":"strokestyle", "widgets":{"color":"color"}},
    {"type":"ptest"},
    {"type":"begin"},
    {"type":"close"},
    {"type":"fill"}
  ],
  "actions":[
    {"type":"cleardown",   "widgets":[]},
    {"type":"console",     "widgets":[{"text":"text"}]},
    {"type":"groupcolor",  "widgets":[{"id":"text"},      {"source":"text"}]},
    {"type":"objectcolor", "widgets":[{"id":"text"},      {"source":"text"}]},
    {"type":"scaledest",   "widgets":[{"filter":"text"},  {"name":"text"},    {"destination":"xy"}, {"rate":"float"}]},
    {"type":"url",         "widgets":[{"url":"filename"}, {"target":"text"}]},
    {"type":"vis",         "widgets":[{"id":"text"},      {"filter":"text"},  {"show":"bool"}]}
  ],
  "widgets":{
    "xy":{"x":"int","y":"int"},
    "text":"string",
    "bool":"boolean",
    "filename":"url",
    "shapename":"shape",
    "color":"rgba"
  },
  "authorsettings":{
  }
}
