{
  "createtypes":["object","image","shape","settings"],
  "settings":{
    "canvasdomname": {"type":"text"},
    "canvaswidth":   {"type":"int"},
    "canvasheight":  {"type":"int"},
    "canvasparent":  {"type":"text"}
  },
  "object":{
    "image":{
      "widgets":[
        {"field":"id",        "type":"text"},
        {"field":"type",      "type":"select", "id":"objecttype"},
        {"field":"order",     "type":"number"},
        {"field":"group",     "type":"arraystrings"},
        {"field":"parent.id", "type":"objlist"},
        {"field":"show",      "type":"bool"},
        {"field":"image",     "type":"imagedata"},
        {"field":"origin",    "type":"select", "id":"objectorigin"},
        {"field":"originxy",  "type":"posxy"},
        {"field":"position",  "type":"posxy"},
        {"field":"atlascell", "type":"cellxy"},
        {"field":"scale",     "type":"scale"},
        {"field":"testp",     "type":"bool"},
        {"field":"clicklist", "type":"actions", "display":"Click/Touch Event"},
        {"field":"draglist",  "type":"actions", "display":"Drag Event"},
        {"field":"droplist",  "type":"actions", "display":"Drop Event"},
        {"field":"truelist",  "type":"actions", "display":"Conditional True"},
        {"field":"falselist", "type":"actions", "display":"Conditional False"}
      ]
    },
    "shape":{
      "widgets":[
        {"field":"id",        "type":"text"},
        {"field":"order",     "type":"number"},
        {"field":"group",     "type":"arraystrings"},
        {"field":"show",      "type":"bool"},
        {"field":"color",     "type":"color"},
        {"field":"shape",     "type":"shapelist"},
        {"field":"origin",    "type":"select", "id":"objectorigin"},
        {"field":"position",  "type":"posxy" },
        {"field":"scale",     "type":"scale"}
      ]
    }
  },
  "particle":{
    "widgets":[
      {"field":"id",                "type":"text"},
      {"field":"order",             "type":"number"},
      {"field":"parent.id",         "type":"objlist"},
      {"field":"on",                "type":"bool"},
      {"field":"image",             "type":"imagedata"},
      {"field":"position",          "type":"posxy"},
      {"field":"emitterSize",       "type":"number"},
      {"field":"emitCounter",       "type":"number"},
      {"field":"emitDirStart",      "type":"number"},
      {"field":"emitDirEnd",        "type":"number"},
      {"field":"emitRate",          "type":"number"},
      {"field":"genType",           "type":"select", "id":"particletype"},
      {"field":"pParams.life.min",  "type":"number", "display":"Life Min"},
      {"field":"pParams.life.max",  "type":"number", "display":"Life Max"},
      {"field":"pParams.fade.in",   "type":"number", "display":"Fade In"},
      {"field":"pParams.fade.out",  "type":"number", "display":"Fade Out"},
      {"field":"pParams.scale.min", "type":"number", "display":"Scale Min"},
      {"field":"pParams.scale.max", "type":"number", "display":"Scale Max"}
    ]
  },
  "anim":{
    "animdata":{
      "widgets":[
        {"field":"id",        "type":"text", "css":"w100"},
        {"field":"autostart", "type":"bool"},
        {"field":"loop",      "type":"bool"},
        {"field":"length",    "type":"number"},
        {"field":"timelist",  "type":"timelist", "display":"Timeline"}
      ]
    }
  },
  "image":{
      "imagedata":{
        "widgets":[
          {"field":"id",         "type":"text",   "css":"w100"},
          {"field":"path",       "type":"select", "id":"pathdata"},
          {"field":"url",        "type":"text"},
          {"field":"atlas",      "type":"bool"},
          {"field":"cellwidth",  "type":"number"},
          {"field":"cellheight", "type":"number"}
        ]
      }
    },
  "shape":{
    "shapedata":{
      "widgets":[
        {"field":"color",        "type":"color"},
        {"field":"defaultcolor", "type":"color"},
        {"field":"selectcolor",  "type":"color"},
        {"field":"widgetbank",   "type":"drawcode"}
      ]
    }
  },
  "drawcode":[
    {"type":"arc",          "widgets":[{"field":"offset",  "type":"posxy"}, {"field":"radius",  "type":"int"}, {"field":"startangle", "type":"int"}, {"field":"endangle", "type":"int"}, {"field":"counterclockwise", "type":"bool"}]},
    {"type":"bcurve",       "widgets":[{"field":"offseta", "type":"posxy"}, {"field":"offsetb", "type":"posxy"},  {"field":"offsetc",    "type":"posxy"}]},
    {"type":"begin",        "widgets":[]},
    {"type":"close",        "widgets":[]},
    {"type":"fill",         "widgets":[]},
    {"type":"fillstyle",    "widgets":[{"field":"color",   "type":"color"}]},
    {"type":"filltext",     "widgets":[{"field":"offset",  "type":"posxy"},{"field":"text", "type":"text"}]},
    {"type":"font",         "widgets":[{"field":"font",    "type":"text"},{"field":"size", "type":"number"}]},
    {"type":"line",         "widgets":[{"field":"offset",  "type":"posxy"}]},
    {"type":"linewidth",    "widgets":[{"field":"width",   "type":"int"}]},
    {"type":"ptest",        "widgets":[]},
    {"type":"rect",         "widgets":[{"field":"offset",  "type":"posxy"}, {"field":"width", "type":"number"},{"field":"height", "type":"number"}]},
    {"type":"strokestyle",  "widgets":[{"field":"color",   "type":"color"}]}
  ],
  "actions":[
    {"type":"cleardown",    "widgets":[]},
    {"type":"console",      "widgets":[{"field":"text",       "type":"text"}]},
    {"type":"conditional",  "widgets":[{"field":"check",      "type":"select", "id":"conditionaltype"},{"field":"itemtocheck",   "type":"objlist"}, {"field":"position",   "type":"objlist"}]},
    {"type":"function",     "widgets":[{"field":"id",         "type":"objlist"},  {"field":"function",  "type":"text"}]},
    {"type":"groupcolor",   "widgets":[{"field":"id",         "type":"text"},     {"field":"source",  "type":"text"}]},
    {"type":"increment",    "widgets":[{"field":"filter",     "type":"select", "id":"filtertype"}, {"field":"id",    "type":"objlist"}, {"field":"prop",    "type":"text"}, {"field":"newvalue",    "type":"text"}, {"field":"rangemin",   "type":"number"}, {"field":"rangemax",   "type":"number"}]},
    {"type":"loadinto",     "widgets":[{"field":"url",        "type":"text"},     {"field":"vari",    "type":"text"}]},
    {"type":"loadpage",     "widgets":[{"field":"url",        "type":"text"},  {"field":"newpage",     "type":"bool"},  {"field":"intodiv",     "type":"bool"}]},
    {"type":"objectcolor",  "widgets":[{"field":"id",         "type":"text"},     {"field":"source",  "type":"text"}]},
    {"type":"playanim",     "widgets":[{"field":"animation",  "type":"anmlist"}]},
    {"type":"set",          "widgets":[{"field":"filter",     "type":"select", "id":"filtertype"}, {"field":"id",    "type":"objlist"}, {"field":"prop",    "type":"text"}, {"field":"newvalue",    "type":"text"}]},
    {"type":"slideobject",  "widgets":[{"field":"id",         "type":"objlist"}, {"field":"constrainx",     "type":"bool"},  {"field":"constrainy",     "type":"bool"}]},
    {"type":"sound",        "widgets":[{"field":"id",         "type":"sndlist"}]},
    {"type":"url",          "widgets":[{"field":"url",        "type":"filename"}, {"field":"target",  "type":"text"}]},
    {"type":"vis",          "widgets":[{"field":"id",         "type":"objlist"},     {"field":"filter",  "type":"select", "id":"filtertype"},   {"field":"show",         "type":"bool"}]}
  ],
  "anims":[
    {"type":"console",      "widgets":[{"field":"starttime",   "type":"number"}, {"field":"text",    "type":"text"}]},
    {"type":"fade",         "widgets":[{"field":"fromcurrent", "type":"bool"},   {"field":"starttime",   "type":"number"}, {"field":"endtime", "type":"number"}, {"field":"id",     "type":"objlist"},  {"field":"filter",  "type":"select", "id":"filtertype"}, {"field":"startalpha",  "type":"number"}, {"field":"endalpha",  "type":"number"}]},
    {"type":"flipbook",     "widgets":[{"field":"starttime",   "type":"number"}, {"field":"id",     "type":"objlist"},  {"field":"filter",  "type":"select", "id":"filtertype"}, {"field":"atlascell.x",  "type":"number"}, {"field":"atlascell.y",  "type":"number"}]},
    {"type":"loadinto",     "widgets":[{"field":"starttime",   "type":"number"}, {"field":"url",         "type":"text"},   {"field":"vari",    "type":"text"}]},
    {"type":"move",         "widgets":[{"field":"fromcurrent", "type":"bool"},   {"field":"starttime",   "type":"number"}, {"field":"endtime", "type":"number"}, {"field":"id",     "type":"objlist"},  {"field":"filter",  "type":"select", "id":"filtertype"}, {"field":"startpos",  "type":"posxy"}, {"field":"endpos",  "type":"posxy"}, {"field":"ease",  "type":"select", "id":"easetype"}]},
    {"type":"play",         "widgets":[{"field":"starttime",   "type":"number"}, {"field":"id",      "type":"animlist"}]},
    {"type":"scale",        "widgets":[{"field":"fromcurrent", "type":"bool"},   {"field":"starttime",   "type":"number"}, {"field":"endtime", "type":"number"}, {"field":"id",     "type":"objlist"},  {"field":"filter",  "type":"select", "id":"filtertype"}, {"field":"startscale",  "type":"number"}, {"field":"endscale",  "type":"number"}]},
    {"type":"sound",        "widgets":[{"field":"starttime",  "type":"number"},  {"field":"id",      "type":"sndlist"}]},
    {"type":"turn",         "widgets":[{"field":"fromcurrent", "type":"bool"},   {"field":"starttime",   "type":"number"}, {"field":"endtime", "type":"number"}, {"field":"id",     "type":"objlist"},  {"field":"filter",  "type":"select", "id":"filtertype"}, {"field":"startrot",  "type":"number"}, {"field":"endrot",  "type":"number"}]},
    {"type":"vis",          "widgets":[{"field":"starttime",   "type":"number"}, {"field":"id",     "type":"objlist"},  {"field":"filter",  "type":"select", "id":"filtertype"}, {"field":"show", "type":"bool"}]}
  ],
  "samples":[
    {"id":"Animation: Atlas",         "url":"anim_atlas.json"},
    {"id":"Animation: Motion Easing", "url":"anim_motionease.json"},
    {"id":"Image: Fade",              "url":"image_fade.json"},
    {"id":"Particle Sample 1",        "url":"particles_sample1.json"},
    {"id":"Particle Rain",            "url":"particles_rain.json"},
    {"id":"lotsostuff",               "url":"multi.json"}
  ],
  "widgets":{
    "bool":"boolean",
    "color":"rgba",
    "filename":"url",
    "number":"number",
    "shapename":"shape",
    "text":"string",
    "xy":{"x":"int","y":"int"}
  },
  "select":{
    "conditionaltype":{"list":["position"]},
    "easetype":{"list":["linear","inExp","outExp","inOutExp","inQuad", "outQuad", "inOutQuad"]},
    "filtertype":{"list":["object", "group","particle"]},
    "objectorigin":{"list":["upper_left", "center"]},
    "objecttype":{"list":["image", "shape"]},
    "particletype":{"list":["burst"]}
  },
  "authorsettings":{
  }
}
