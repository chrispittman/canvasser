{
  "createtypes":["object","image","shape","settings"],
  "settings":[
    {"type":"canvaswidth",  "widgets":[{"field":"size",   "type":"int"}]},
    {"type":"canvasheight", "widgets":[{"field":"size",   "type":"int"}]},
    {"type":"canvasparent", "widgets":[{"field":"parent", "type":"text"}]}
  ],
  "object":{
    "image":{
      "widgets":[
        {"field":"id",        "type":"text"},
        {"field":"group",     "type":"arraystrings"},
        {"field":"parent.id", "type":"objlist"},
        {"field":"show",      "type":"bool"},
        {"field":"image",     "type":"imagedata"},
        {"field":"origin",    "type":"select", "id":"objectorigin"},
        {"field":"position",  "type":"posxy"},
        {"field":"scale",     "type":"scale"},
        {"field":"testp",     "type":"bool"},
        {"field":"clicklist", "type":"actions"},
        {"field":"draglist",  "type":"actions"},
        {"field":"droplist",  "type":"actions"},
        {"field":"truelist",  "type":"actions"}
      ]
    },
    "shape":{
      "widgets":[
        {"field":"id",        "type":"text"},
        {"field":"group",     "type":"arraystrings"},
        {"field":"show",      "type":"bool"},
        {"field":"shape",     "type":"shapedata"},
        {"field":"position",  "type":"posxy" }
      ]
    }
  },
  "image":{
    "imagedata":{
      "widgets":[
        {"field":"id",        "type":"text"},
        {"field":"url",       "type":"filename"}
      ]
    }
  },
  "shape":[
    {"type":"shapedata", "widgets":{"id":"text", "color":"color", "defaultcolor":"color", "selectcolor":"color"},"widgetbank":["drawcode"]}
  ],
  "drawcode":[
    {"type":"line",        "widgets":{"offset":"xy"}},
    {"type":"arc",         "widgets":{"offset":"xy", "radius":"int","startangle":"int", "endangle":"int", "counterclockwise":"bool"}},
    {"type":"bcurve",      "widgets":{"offseta":"xy", "offsetb":"xy", "offsetc":"xy"}},
    {"type":"linewidth",   "widgets":{"width":"int"}},
    {"type":"fillStyle",   "widgets":{"color":"color"}},
    {"type":"strokestyle", "widgets":{"color":"color"}},
    {"type":"ptest"},
    {"type":"begin"},
    {"type":"close"},
    {"type":"fill"}
  ],
  "actions":[
    {"type":"cleardown",   "widgets":[]},
    {"type":"console",     "widgets":[{"field":"text",   "type":"text"}]},
    {"type":"groupcolor",  "widgets":[{"field":"id",     "type":"text"},     {"field":"source",  "type":"text"}]},
    {"type":"objectcolor", "widgets":[{"field":"id",     "type":"text"},     {"field":"source",  "type":"text"}]},
    {"type":"scaledest",   "widgets":[{"field":"filter", "type":"text"},     {"field":"name",    "type":"text"},   {"field":"destination",  "type":"xy"}, {"field":"rate",  "type":"float"}]},
    {"type":"url",         "widgets":[{"field":"url",    "type":"filename"}, {"field":"target",  "type":"text"}]},
    {"type":"vis",         "widgets":[{"field":"id",     "type":"text"},     {"field":"filter",  "type":"select", "id":"filtertype"},   {"field":"show",         "type":"bool"}]}
  ],
  "widgets":{
    "xy":{"x":"int","y":"int"},
    "text":"string",
    "bool":"boolean",
    "filename":"url",
    "shapename":"shape",
    "color":"rgba"
  },
  "select":{
    "objectorigin":{"list":["upper_left", "center"]},
    "filtertype":{"list":["object", "group"]}
  },
  "authorsettings":{
  }
}
