{
  "createtypes":["object","image","shape","settings"],
  "settings":{
    "canvasdomname": {"type":"text"},
    "canvaswidth":   {"type":"int"},
    "canvasheight":  {"type":"int"},
    "canvasparent":  {"type":"text"}
  },
  "object":{
    "image":{
      "widgets":[
        {"field":"id",        "type":"text"},
        {"field":"group",     "type":"arraystrings"},
        {"field":"parent.id", "type":"objlist"},
        {"field":"show",      "type":"bool"},
        {"field":"image",     "type":"imagedata"},
        {"field":"origin",    "type":"select", "id":"objectorigin"},
        {"field":"position",  "type":"posxy"},
        {"field":"scale",     "type":"scale"},
        {"field":"testp",     "type":"bool"},
        {"field":"clicklist", "type":"actions", "display":"Click/Touch Event"},
        {"field":"draglist",  "type":"actions", "display":"Drag Event"},
        {"field":"droplist",  "type":"actions", "display":"Drop Event"},
        {"field":"truelist",  "type":"actions", "display":"Conditional True"},
        {"field":"falselist", "type":"actions", "display":"Conditional False"}
      ]
    },
    "shape":{
      "widgets":[
        {"field":"id",        "type":"text"},
        {"field":"group",     "type":"arraystrings"},
        {"field":"show",      "type":"bool"},
        {"field":"shape",     "type":"shapedata"},
        {"field":"position",  "type":"posxy" }
      ]
    }
  },
  "image":{
    "imagedata":{
      "widgets":[
        {"field":"id",        "type":"text"},
        {"field":"url",       "type":"filename"}
      ]
    }
  },
  "shape":[
    {"type":"shapedata", "widgets":{"id":"text", "color":"color", "defaultcolor":"color", "selectcolor":"color"},"widgetbank":["drawcode"]}
  ],
  "drawcode":[
    {"type":"line",        "widgets":{"offset":"xy"}},
    {"type":"arc",         "widgets":{"offset":"xy", "radius":"int","startangle":"int", "endangle":"int", "counterclockwise":"bool"}},
    {"type":"bcurve",      "widgets":{"offseta":"xy", "offsetb":"xy", "offsetc":"xy"}},
    {"type":"linewidth",   "widgets":{"width":"int"}},
    {"type":"fillStyle",   "widgets":{"color":"color"}},
    {"type":"strokestyle", "widgets":{"color":"color"}},
    {"type":"ptest"},
    {"type":"begin"},
    {"type":"close"},
    {"type":"fill"}
  ],
  "actions":[
    {"type":"cleardown",    "widgets":[]},
    {"type":"console",      "widgets":[{"field":"text",   "type":"text"}]},
    {"type":"conditional",  "widgets":[{"field":"check",  "type":"select", "id":"conditionaltype"},{"field":"itemtocheck",   "type":"objlist"}, {"field":"position",   "type":"objlist"}]},
    {"type":"loadinto",     "widgets":[{"field":"url",    "type":"text"},     {"field":"vari",    "type":"text"}]},
    {"type":"destposition", "widgets":[{"field":"id",     "type":"objlist"},  {"field":"filter",  "type":"select", "id":"filtertype"}, {"field":"destination",  "type":"posxy"}, {"field":"rate",   "type":"number"}]},
    {"type":"groupcolor",   "widgets":[{"field":"id",     "type":"text"},     {"field":"source",  "type":"text"}]},
    {"type":"loadpage",     "widgets":[{"field":"url",    "type":"text"}]},
    {"type":"objectcolor",  "widgets":[{"field":"id",     "type":"text"},     {"field":"source",  "type":"text"}]},
    {"type":"scaledest",    "widgets":[{"field":"filter",  "type":"select", "id":"filtertype"}, {"field":"id",    "type":"objlist"}, {"field":"destination",    "type":"number"},  {"field":"showbefore",  "type":"bool"},   {"field":"hideafter",  "type":"bool"}, {"field":"rate",  "type":"number"}]},
    {"type":"set",          "widgets":[{"field":"filter",  "type":"select", "id":"filtertype"}, {"field":"id",    "type":"objlist"}, {"field":"prop",    "type":"text"}, {"field":"newvalue",    "type":"text"}]},
    {"type":"slideobject",  "widgets":[{"field":"id",     "type":"objlist"}, {"field":"constrainx",     "type":"bool"},  {"field":"constrainy",     "type":"bool"}]},
    {"type":"url",          "widgets":[{"field":"url",    "type":"filename"}, {"field":"target",  "type":"text"}]},
    {"type":"vis",          "widgets":[{"field":"id",     "type":"text"},     {"field":"filter",  "type":"select", "id":"filtertype"},   {"field":"show",         "type":"bool"}]}
  ],
  "widgets":{
    "bool":"boolean",
    "color":"rgba",
    "filename":"url",
    "number":"number",
    "shapename":"shape",
    "text":"string",
    "xy":{"x":"int","y":"int"}
  },
  "select":{
    "objectorigin":{"list":["upper_left", "center"]},
    "filtertype":{"list":["object", "group"]},
    "conditionaltype":{"list":["position"]}
  },
  "authorsettings":{
  }
}
